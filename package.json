{
  "name": "swapcat-subgraph",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/ subgraph.yaml",
    "build": "graph build",
    "deploy:eth": "yarn codegen:eth && graph deploy --product hosted-service --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ realtoken-thegraph/swapcat-eth",
    "deploy:xdai": "yarn codegen:xdai && graph deploy --product hosted-service --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ realtoken-thegraph/swapcat-xdai",
    "deploy": "yarn deploy:eth && yarn deploy:xdai",
    "script": "ts-node scripts/createSubgraph.ts",
    "precodegen:xdai": "rimraf generated && yarn script xdai",
    "precodegen:eth": "rimraf generated && yarn script eth",
    "codegen:xdai": "graph codegen",
    "codegen:eth": "graph codegen"
  },
  "repository": {
